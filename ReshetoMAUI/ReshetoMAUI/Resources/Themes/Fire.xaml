<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ReshetoMAUI.Resources.Themes.Fire">

    <Color x:Key="Background">#f29955</Color>
    <Color x:Key="Button">#e25822</Color>
    <Color x:Key="LabelBack">#ffffff</Color>
    <Color x:Key="LabelText">#3d2856</Color>

    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource Background}"/>
    <SolidColorBrush x:Key="ButtonBrush" Color="{StaticResource Button}"/>
    <SolidColorBrush x:Key="LabelBackBrush" Color="{StaticResource LabelBack}"/>
    <SolidColorBrush x:Key="LabelTextBrush" Color="{StaticResource LabelText}"/>

</ResourceDictionary>