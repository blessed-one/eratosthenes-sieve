<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ReshetoMAUI.Resources.Themes.Nature">

    <Color x:Key="Good">#00ff7f</Color>
    <Color x:Key="Bad">#b8860b</Color>
    <Color x:Key="Unknown">#5c0168</Color>

    <SolidColorBrush x:Key="GoodBrush" Color="{StaticResource Good}"/>
    <SolidColorBrush x:Key="BadBrush" Color="{StaticResource Bad}"/>
    <SolidColorBrush x:Key="UnknownBrush" Color="{StaticResource Unknown}"/>

</ResourceDictionary>