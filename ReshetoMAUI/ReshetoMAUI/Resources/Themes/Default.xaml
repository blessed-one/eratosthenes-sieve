<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ReshetoMAUI.Resources.Themes.Default">

    <Color x:Key="Good">#00ff00</Color>
    <Color x:Key="Bad">#ff0000</Color>
    <Color x:Key="Unknown">Transparent</Color>

    <SolidColorBrush x:Key="GoodBrush" Color="{StaticResource Good}"/>
    <SolidColorBrush x:Key="BadBrush" Color="{StaticResource Bad}"/>
    <SolidColorBrush x:Key="UnknownBrush" Color="{StaticResource Unknown}"/>

</ResourceDictionary>